{
  "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      ["env", {
        "targets": {
          "browsers": ["last 2 Chrome versions"]
        }
      }]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties", { 
        "loose": true 
      },
      "@babel/plugin-transform-runtime", {
        "corejs": 2
      }
    ]
  ]
}